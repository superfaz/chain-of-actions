{
  "name": "chain-of-actions",
  "version": "0.9.4",
  "author": "Francois Karman",
  "type": "module",
  "license": "MIT",
  "description": "Railway error management implementation in Typescript",
  "keywords": [
    "typescript",
    "error-handling",
    "railway",
    "railway-oriented-programming",
    "chain-of-responsibility"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/superfaz/chain-of-actions.git"
  },
  "exports": {
    ".": "./dist/esm/index.js",
    "./*": "./dist/esm/*.js"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist/",
    "build:esm": "tsc -p tsconfig.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && renamer --find js --replace cjs ./dist/cjs/*",
    "build": "pnpm run build:esm && pnpm run build:cjs && pnpm dlx publint",
    "lint": "prettier --check . && eslint .",
    "test": "vitest",
    "all": "pnpm run lint && pnpm run build && pnpm run test run"
  },
  "packageManager": "pnpm@9.12.1+sha512.e5a7e52a4183a02d5931057f7a0dbff9d5e9ce3161e33fa68ae392125b79282a8a8a470a51dfc8a0ed86221442eb2fb57019b0990ed24fab519bf0e1bc5ccfc4",
  "dependencies": {
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@vitest/coverage-v8": "^2.1.2",
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "renamer": "^5.0.2",
    "rimraf": "^6.0.1",
    "typescript-eslint": "^8.8.1",
    "vitest": "^2.1.2"
  }
}
