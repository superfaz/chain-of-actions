{
  "name": "chain-of-actions",
  "version": "0.11.1",
  "author": "Francois Karman",
  "type": "module",
  "license": "MIT",
  "description": "Railway error management implementation in Typescript",
  "keywords": [
    "typescript",
    "error-handling",
    "railway",
    "railway-oriented-programming",
    "chain-of-responsibility"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/superfaz/chain-of-actions.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "pkgroll && pnpm dlx publint",
    "lint": "prettier --check . && eslint .",
    "test": "vitest",
    "all": "pnpm run lint && pnpm run build && pnpm run test run"
  },
  "packageManager": "pnpm@9.12.3+sha512.cce0f9de9c5a7c95bef944169cc5dfe8741abfb145078c0d508b868056848a87c81e626246cb60967cbd7fd29a6c062ef73ff840d96b3c86c40ac92cf4a813ee",
  "dependencies": {
    "typescript": "^5.6.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@vitest/coverage-v8": "^2.1.5",
    "eslint": "^9.14.0",
    "pkgroll": "^2.5.1",
    "prettier": "^3.3.3",
    "renamer": "^5.0.2",
    "rimraf": "^6.0.1",
    "typescript-eslint": "^8.14.0",
    "vitest": "^2.1.5"
  }
}
