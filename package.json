{
  "name": "chain-of-actions",
  "version": "0.11.1",
  "author": "Francois Karman",
  "type": "module",
  "license": "MIT",
  "description": "Railway error management implementation in Typescript",
  "keywords": [
    "typescript",
    "error-handling",
    "railway",
    "railway-oriented-programming",
    "chain-of-responsibility"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/superfaz/chain-of-actions.git"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    }
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "pkgroll && pnpm dlx publint",
    "lint": "prettier --check . && eslint .",
    "test": "vitest",
    "all": "pnpm run lint && pnpm run build && pnpm run test run"
  },
  "packageManager": "pnpm@9.15.0+sha512.76e2379760a4328ec4415815bcd6628dee727af3779aaa4c914e3944156c4299921a89f976381ee107d41f12cfa4b66681ca9c718f0668fa0831ed4c6d8ba56c",
  "dependencies": {
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@vitest/coverage-v8": "^2.1.8",
    "eslint": "^9.16.0",
    "pkgroll": "^2.5.1",
    "prettier": "^3.4.2",
    "renamer": "^5.0.2",
    "rimraf": "^6.0.1",
    "typescript-eslint": "^8.17.0",
    "vitest": "^2.1.8"
  }
}
